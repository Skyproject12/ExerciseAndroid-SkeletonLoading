<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite">

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout5"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraint_akun"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorWhite">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraint_detail"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/logo_detail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/activity_margin_duaempat"
                    android:layout_marginTop="@dimen/activity_margin_duaempat"
                    android:src="@drawable/ic_detail"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/image_detail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/activity_margin_enambelas"
                    android:layout_marginRight="@dimen/activity_margin_enambelas"
                    android:src="@drawable/ic_next"
                    app:layout_constraintBottom_toBottomOf="@+id/text_detail"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/text_detail" />

                <TextView
                    android:id="@+id/text_detail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/activity_margin_delapan"
                    android:layout_marginLeft="@dimen/activity_margin_delapan"
                    android:text="@string/lihat_detail_profil"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@+id/logo_detail"
                    app:layout_constraintStart_toEndOf="@+id/logo_detail"
                    app:layout_constraintTop_toTopOf="@+id/logo_detail" />

                <View
                    android:id="@+id/view2"
                    android:layout_width="0dp"
                    android:layout_height="0.6dp"
                    android:layout_marginStart="@dimen/activity_margin_enambelas"
                    android:layout_marginTop="@dimen/activity_margin_enambelas"
                    android:layout_marginEnd="@dimen/activity_margin_enambelas"
                    android:background="@color/colorView"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/text_detail" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraint_profile"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraint_detail">

                <ImageView
                    android:id="@+id/logo_edit_profil"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/activity_margin_duaempat"
                    android:layout_marginTop="@dimen/activity_margin_duaempat"
                    android:src="@drawable/ic_edit"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/image_edit_profil"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/activity_margin_enambelas"
                    android:layout_marginRight="@dimen/activity_margin_enambelas"
                    android:src="@drawable/ic_next"
                    app:layout_constraintBottom_toBottomOf="@+id/text_edit_profil"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/text_edit_profil" />

                <TextView
                    android:id="@+id/text_edit_profil"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/activity_margin_delapan"
                    android:layout_marginLeft="@dimen/activity_margin_delapan"
                    android:text="@string/sunting_profil"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@+id/logo_edit_profil"
                    app:layout_constraintStart_toEndOf="@+id/logo_edit_profil"
                    app:layout_constraintTop_toTopOf="@+id/logo_edit_profil" />

                <View
                    android:id="@+id/view3"
                    android:layout_width="0dp"
                    android:layout_height="0.6dp"
                    android:layout_marginStart="@dimen/activity_margin_enambelas"
                    android:layout_marginTop="@dimen/activity_margin_enambelas"
